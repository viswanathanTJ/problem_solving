{
  "code-runner.executorMap": {
    "python": "cat input.txt | uv run $fullFileName",
    "java": "cd $dir && javac $fileName && cat $workspaceRoot/input.txt | java $fileNameWithoutExt && cd $workspaceRoot",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && cat $workspaceRoot/input.txt | $dir$fileNameWithoutExt && cd $workspaceRoot",
  },
  "java.project.sourcePaths": [],
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
}